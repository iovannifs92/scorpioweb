                                    + "<a href=/Personas/PresentacionPeriodicaPersona/" + result[i].idPersona + " style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-default'><i class='fa fa-calendar'></i> Presentaciónes</a>"
                                    + "<a href=/Personas/MenuEdicion/" + result[i].idPersona + " style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-success'><i class='fa fa-pencil'></i> Editar</a></td>"
                                    + "<td> <button style='margin-top: 10px;' class='btn btn-primary' onclick='candado2(" + result[i].idPersona + ",true)'><i id='des' name='des' class='fa fa-unlock'></i></button></td>"
                                    + "</tr>";
                            } else {
                                row = row
                                    + "<tr valign='middle'>"
                                    + "<td>" + result[i].idPersona + "</td>"
                                    + "<td>" + result[i].nombreCompleto + "</td>"
                                    + "<td>" + result[i].supervisor + "</td>"
                                    + "<th>" + date + "</th>"
                                    + "<td> <a  href=/Personas/Details/" + result[i].idPersona + "  style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-info'><i class='fa fa-eye'></i> Detalles</a>"
                                    + "<a href=/Personas/Procesos/" + result[i].idPersona + "  style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-primary'><i class='fa fa-cogs'></i> Procesos</a>"
                                    + "<a href=/Personas/PresentacionPeriodicaPersona/" + result[i].idPersona + " style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-default'><i class='fa fa-calendar'></i> Presentaciónes</a>"
                                    + "<a href=/Personas/MenuEdicion/" + result[i].idPersona + " style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-success'><i class='fa fa-pencil'></i> Editar</a></td>"
                                    + "<td> <button style='margin-top: 10px; margin-left:5px; margin-rigth:5px;' class='btn btn-danger' onclick='candado2(" + result[i].idPersona + ",false)'><i id='des' name='des' class='fa fa-lock'></i></button></td>"
                                    + "</tr>";
                         }
                    }
                    if (row != '') {
                        $('#tblPersona').append(row);
                    }
                }
            },
            error: function (msg) {
            }
        });
    };
    //#endregion
</script>

