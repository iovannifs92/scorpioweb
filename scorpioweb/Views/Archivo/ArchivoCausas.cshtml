@{ ViewBag.Title = "";
    Layout = null; }
<script type="text/javascript">
     showModal = (url, title, id) => { 
        $.ajax({
            type: "GET",
            url: url + "?id="+id,
            success: function (res) {
                $("#form-modal .modal-body").html(res);
                $("#form-modal .modal-title").html(title);
                $("#form-modal").modal('show');
            },
            //data: {
            //    datosBitacora: [IdFracciones],
            //},
            error: function (res) {

            }
        })
    }
</script>



@foreach (var item in ViewData["tienearchivo"] as IEnumerable<scorpioweb.Models.ArchivoControlPrestamo>)
{
    <a href="~/Expedientes/@Html.DisplayFor(modelItem => item.archivoVM.Urldocumento)" class="btn btn-primary btn-lg btn-block" role="button" aria-pressed="true" target="_blank" rel="noopener noreferrer">@item.archivoregistroVM.CausaPenal</a>
}
