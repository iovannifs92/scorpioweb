@model IEnumerable<scorpioweb.Models.PersonaclsViewModal>

@{
    ViewBag.Title = "Detalles";
    Layout = "/Views/Shared/_Layout.cshtml";
}
<link rel="stylesheet" type="text/css" href="~/css/tabla.css">

@* Necesario para los modales en el servidor *@
<script src="~/js/jquery-1.11.1.min.js"></script>
<script>


    $(document).ready(function () {

        if ($("#selectINE").val() == "NO") {
            $("#divINE").css("display", "block");
        }

        if ($("#selectAsesoria").val() == "SI") {
            $("#divAsesoria").css("display", "block");
        }
        $("#inputRelacionPH").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'RelacionPH');
            }
        });

        $("#inputRelacionMPH").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'RelacionMPH');
            }
        });
        $("#inputSienteEmocionalmente").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'SienteEmocionalmente');

            }
        });

        $("#inputINE").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'INE');
            }
        });
        $("#inputAsesoria").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'Asesoria');
            }
        });

        $("#selectApoyoPsicologico").change(function () {
            $("#selectApoyoPsicologico").blur(function () {
                var inputValue = $(this).val();
                if (inputValue != "") {
                    AjaxVinculacion(inputValue, 'selectApoyoPsicologico');
                }
            });
        });

        $("#selectINE").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'selectINE');
            }
        });
        $("#selectAsesoria").blur(function () {
            var inputValue = $(this).val();
            if (inputValue != "") {
                AjaxVinculacion(inputValue, 'selectAsesoria');
            }
        });

        $("#selectINE").change(function () {
            if ($(this).val() === "NO") {
                $("#divINE").css("display", "block");
            } else {
                $("#divINE").css("display", "none");
                $("#inputINE").val("NA");
            }
        });


        $("#selectAsesoria").change(function () {
            if ($(this).val() === "SI") {
                $("#divAsesoria").css("display", "block");

            } else {
                $("#divAsesoria").css("display", "none");
                $("#inputAsesoria").val("NA");
            }
        });

        function AjaxVinculacion(valor, NombreCampo) {
            var idPersonaCL = document.getElementById("idPersonaCL").value;

            $.ajax({
                dataType: "json",
                url: "/Personacls/PsicologiaVinculacion",
                type: "POST",
                data: { valor: valor, NombreCampo: NombreCampo, idPersonaCL: idPersonaCL },
                success: function (response) {
                    console.log("Response from server:", response);
                },
                error: function (xhr, status, error) {
                    console.error("Error:", error);
                }
            });
        };
    });




    showModal = (url, title, id) => {
        $.ajax({
            type: "GET",
            url: url + "?id=" + id,
            success: function (res) {
                $("#form-modal .modal-body").html(res);
                $("#form-modal .modal-title").html(title);
                $("#form-modal").modal('show');
            },
            error: function (res) {
            
            }
        })
    }
</script>


